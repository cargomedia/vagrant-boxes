{
  "builders": [
    {
      "boot_command": [
        "<esc><wait>",
        "install <wait>",
        "preseed/url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg <wait>",
        "debian-installer=en_US ",
        "auto ",
        "locale=en_US ",
        "kbd-chooser/method=de ",
        "netcfg/get_hostname={{ .Name }} ",
        "netcfg/get_domain=vagrantup.com ",
        "fb=false ",
        "debconf/frontend=noninteractive ",
        "console-setup/ask_detect=false ",
        "console-keymaps-at/keymap=de ",
        "keyboard-configuration/xkb-keymap=de ",
        "<enter><wait>"
      ],
      "boot_wait": "3s",
      "disk_size": 30000,
      "guest_os_type": "Debian_64",
	"guest_additions_url": "file:///Users/cargomedia/Projects/vagrant-boxes/iso/VBoxGuestAdditions_4.2.16.iso",
      "http_directory": "http",
      "iso_checksum": "cc509122349a27bf1958e1ea0ce4065d",
      "iso_checksum_type": "md5",
      "iso_url": "http://s3.cargomedia.ch/vagrant-boxes/iso/debian-6.0.7-amd64-netinst.iso",
		"iso_url": "file:///Users/cargomedia/Projects/vagrant-boxes/iso/debian-6.0.7-amd64-netinst.iso",
      "shutdown_command": "echo 'halt -p' > shutdown.sh; echo 'vagrant'|sudo -S bash 'shutdown.sh'",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "10000s",
      "type": "virtualbox",
		"guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
		"virtualbox_version_file": ".vbox_version",
      "vboxmanage": [
		  [
			  "modifyvm",
			  "{{.Name}}",
			  "--memory",
			  "512"
		  ],
		  [
			  "modifyvm",
			  "{{.Name}}",
			  "--cpus",
			  "2"
		  ]
	  ],
      "vm_name": "debian-6-amd64"
    }
  ],
  "post-processors": [
    {
      "output": "debian-6-amd64.box",
      "type": "vagrant"
    }
  ],
  "provisioners": [
    {
      "override": {
        "virtualbox": {
			"execute_command": "echo 'vagrant'|sudo -S bash '{{.Path}}'"
		},
        "vmware": {}
      },
      "scripts": [
        "scripts/base.sh",
        "scripts/vagrant.sh",
        "scripts/virtualbox.sh",
		"scripts/ruby.sh",
		"scripts/puppet.sh",
		"scripts/chef.sh",
		"scripts/cleanup-virtualbox.sh",
        "scripts/cleanup.sh",
        "scripts/zerodisk.sh"
      ],
      "type": "shell"
    }
  ]
}

